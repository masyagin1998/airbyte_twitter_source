{
  "streams": [
    {
      "stream": {
        "name": "tweets",
        "json_schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "title": "Twitter tweets",
          "description": "Twitter tweets schema",
          "type": "object",
          "required": [
            "user_id",
            "message"
          ],
          "properties": {
            "user_id": {
              "type": "string"
            },
            "message": {
              "type": "string"
            },
            "ticket_id": {
              "type": "string"
            },
            "score": {
              "type": "integer"
            },
            "version": {
              "type": "string"
            },
            "ds": {
              "type": "string",
              "format": "date-time"
            },
            "source": {
              "type": "string"
            },
            "lang": {
              "type": "string"
            },
            "country": {
              "type": "string"
            }
          }
        },
        "supported_sync_modes": [
          "full_refresh",
          "incremental"
        ]
      },
      "sync_mode": "incremental",
      "destination_sync_mode": "append"
    }
  ]
}